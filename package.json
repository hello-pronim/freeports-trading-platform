    "test:organization": "yarn run copy:organization:env && react-scripts test",
    "eject": "react-scripts eject",
    "copy:clearer:env": "copyfiles -f env/clearer/.* ./",
    "copy:organization:env": "copyfiles -f env/organization/.*"
  },
